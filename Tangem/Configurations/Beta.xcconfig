// Configuration settings file format documentation can be found at:
// https://help.apple.com/xcode/#/dev745c5c974

#include "Common.xcconfig"

// Config name
ENVIRONMENT_NAME = Beta

// Will use in TangemApp as BUNDLE_IDENTIFIER
APP_BUNDLE_IDENTIFIER = $(APP_BUNDLE_IDENTIFIER_COMMON)$(ENVIRONMENT_NAME)

// Path to *.entitlements file
ENTITLEMENTS_PATH = Tangem/Configurations/TangemBeta.entitlements

// Overriding (do NOT change key name)
// app name
PRODUCT_NAME = $(PRODUCT_NAME_COMMON) $(ENVIRONMENT_NAME)

// Additional swift compiler flags
SWIFT_ACTIVE_COMPILATION_CONDITIONS = BETA ALPHA_OR_BETA

// Additional clang compiler flags
GCC_PREPROCESSOR_DEFINITIONS = BETA=1 ALPHA_OR_BETA=1
